<!DOCTYPE HTML>
<html>
<head>
	<title>{% block title %}{{ config.site_name }}{% endblock %}</title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<meta name="description" content="" />

	<script src="{{ asset('js/plugins/modernizr-respond.min.js') }}"></script>
	
	<link rel="stylesheet" href="{{ asset('css/main.css') }}" />

	{% block head %}{% endblock %}
</head>
<body class="{% block body_class %}{% endblock %}">
{% include '@blocks/modules/svgs.twig' %}

<div class="wrapper">

{% block header %}
	{% include '@blocks/modules/header.twig' %}
{% endblock header %}

{% block nav %}
	{% include '@blocks/modules/nav.twig' %}
{% endblock nav %}


{% block main %}{% endblock %}

</div>{# end .wrapper #}

{% block footer %}
	{% include '@blocks/modules/footer.twig' %}
{% endblock footer %}

<script src="{{ asset('js/plugins/jquery-1.11.1.min.js') }}"></script>
<script src='https://cdn.firebase.com/js/client/2.2.1/firebase.js'></script>
<script src="{{ asset('js/plugins/d3.v3.min.js') }}"></script>
<script src="{{ asset('js/plugins/nv.d3.min.js') }}"></script>
<script src="{{ asset('js/plugins/moment.js') }}"></script>

<script src="{{ asset('js/all.js') }}"></script>

</body>
</html>